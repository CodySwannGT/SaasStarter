{
  "name": "frontend",
  "version": "0.134.20",
  "main": "index",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "expo start -c",
    "dev:web": "expo start -c --web",
    "dev:android": "expo start -c --android",
    "dev:ios": "expo start -c --ios",
    "start:local": "cp .env.localhost .env.local && expo start -c",
    "start:dev": "cp .env.development .env.local && expo start -c",
    "start:staging": "cp .env.staging .env.local && expo start -c",
    "start:production": "cp .env.production .env.local && expo start",
    "prepare": "yarn husky install",
    "watch:css": "npx tailwindcss --watch -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
    "generate:css": "npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
    "generate:component": "./scripts/addComponent.sh",
    "generate:feature": "./scripts/addFeature.sh",
    "generate:types:local": "DOTENV_CONFIG_PATH=.env.localhost graphql-codegen -r dotenv/config --config codegen.ts",
    "generate:types:dev": "DOTENV_CONFIG_PATH=.env.development graphql-codegen -r dotenv/config --config codegen.ts",
    "generate:types:staging": "DOTENV_CONFIG_PATH=.env.staging graphql-codegen -r dotenv/config --config codegen.ts",
    "generate:types:prod": "DOTENV_CONFIG_PATH=.env.production graphql-codegen -r dotenv/config --config codegen.ts",
    "build": "expo export",
    "build:web": "expo export --platform web -c",
    "postinstall": "patch-package && yarn generate:css",
    "lint": "eslint . --fix",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests implemented yet\" && exit 0",
    "test:e2e": "echo \"Playwright tests will be implemented in Q4\" && exit 0",
    "clean": "rm -rf node_modules .turbo .expo",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
